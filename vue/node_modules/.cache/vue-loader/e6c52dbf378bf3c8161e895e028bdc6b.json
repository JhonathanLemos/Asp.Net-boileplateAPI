v-alert\n    >\n    <!-- DIALOG CONFIRM DELETE -->\n    <v-dialog v-model=\"dialogDeleteConfirm\" persistent max-width=\"450px\">\n      <div class=\"white pa-8 d-flex flex-column align-center text-center\">\n        <v-icon style=\"font-size: 60px\" color=\"myApi-red\"\n          >mdi-alert-circle-outline</v-icon\n        >\n\n        <div class=\"d-flex align-start\">\n          <h4 class=\"myApi-neutral--text text--var-1 pb-5 pt-5\">\n            Deseja excluir este registro? <br>\n            Esta ação não poderá ser desfeita. Deseja continuar?\n          </h4>\n        </div>\n\n        <div class=\"d-flex flex-row justify-end\">\n          <v-btn\n            rounded\n            large\n            text\n            class=\"mr-4\"\n            @click=\"dialogDeleteConfirm = false\"\n            >CANCELAR</v-btn\n          >\n\n          <v-btn\n            rounded\n            large\n            class=\"myApi-red white--text\"\n            @click=\"deleteItem(2)\"\n            :loading=\"loadingBtnDelete\"\n            >PROSSEGUIR</v-btn\n          >\n        </div>\n      </div>\n    </v-dialog>\n\n    <top-section></top-section>\n\n    <actions-table\n      :params=\"getRouterData\"\n      :disableButton=\"!editPermission\"\n      v-on:clickAdd=\"$router.push(`usuarios/Add`)\"\n      labelSearch=\"Pesquisar por Nome ou Perfil\"\n    ></actions-table>\n\n    <div class=\"flex-grow-1 rounded-lg pa-4 white elevation-5\">\n      <!-- DATA TABLE -->\n      <v-data-table\n        :key=\"key\"\n        disable-sort\n        :headers=\"headers\"\n        :items=\"rows\"\n        :options.sync=\"options\"\n        :page=\"page\"\n        :server-items-length=\"totalItems\"\n        noDataText=\"Nenhum registro encontrado\"\n        :loading=\"tableLoading\"\n        loading-text=\"Aguarde, carregando usuários...\"\n        :footer-props=\"{\n          itemsPerPageOptions: [10, 50, 100],\n          itemsPerPageText: 'Itens por Página',\n        }\"\n      >\n        <!-- Icone Manutenção -->\n        <template v-slot:[`item.col1`]=\"{ item }\">\n          <div v-if=\"editPermission\" class=\"d-flex flex-nowrap\">\n            <v-btn\n              icon\n              @click=\"$router.push(`/home/usuarios/edit/${item.col1}`)\"\n            >\n              <v-icon> mdi-pencil </v-icon>\n            </v-btn>\n\n            <v-btn icon @click=\"deleteItem(1, item.col1)\">\n              <v-icon> mdi-delete </v-icon>\n            </v-btn>\n          </div>\n          <div v-else class=\"d-flex flex-nowrap\">\n            <v-btn icon @click=\"$router.push(`/home/usuarios/edit/${item.col1}`)\">\n              <v-icon> mdi-eye </v-icon>\n            </v-btn>\n          </div>\n        </template>\n\n        <template v-slot:[`item.col4`]=\"{ item }\">\n          {{ item.col4 | userProfile }}\n        </template>\n\n        <template v-slot:[`item.col5`]=\"{ item }\">\n          <div style=\"max-width:200px ; width: 90% !important;  \" class=\"testeClass\">\n            {{ item.col5 }}\n          </div>\n        </template>\n\n        <!-- Status -->\n        <template v-slot:[`item.col6`]=\"{ item }\">\n          <v-chip :color=\"item.col6.color\" outlined style=\"font-weight: 500\">\n            {{ item.col6.text }}\n          </v-chip>\n        </template>\n      </v-data-table>\n    </div>\n  </div>\n</template>\n\n<script>\nimport TopSection from \"@/components/TopSection.vue\";\nimport ActionsTable from \"@/components/ActionsTable.vue\";\nimport getRouterData from \"@/mixins/getRouterData.js\";\nimport dataTable from \"@/mixins/dataTable.js\";\nimport crudController from \"@/mixins/crudController.js\";\nimport validationRules from \"../../mixins/validationRules\";\nimport { mapActions } from \"vuex\";\n\nexport default {\n  data: () => ({\n    headers: [\n      { text: \"\", value: \"col1\", width: \"10%\" },\n      { text: \"Nome\", value: \"col2\", width: \"10%\" },\n      { text: \"E-mail\", value: \"col3\", width: \"20%\" },\n      { text: \"Perfil\", value: \"col4\", width: \"20%\" },\n      { text: \"Vínculo\", value: \"col5\" },\n      { text: \"Status\", value: \"col6\", width: \"10%\" },\n    ],\n    rows: [],\n  }),\n\n  computed: {\n    editPermission() {\n      return this.hasPermission(this.$PermissionNames.permissionUserEdit);\n    },\n  },\n\n  methods: {\n    ...mapActions(\"ModuleGrupoClientes\", {\n      getAllSelect: \"getAllSelect\",\n      createGroupCustomers: \"create\",\n      getGroupById: \"getById\",\n    }),\n\n    // setGroups(customers) {\n    //   var contemGrupo = customers.filter((x) => x.customerGroup != null);\n    //   var naoContemGrupo = customers.filter((x) => x.customerGroup == null);\n    //   customers.vinculos = [];\n\n    //   contemGrupo.forEach((x) => {\n    //     var jaExiste = customers.vinculos.filter(\n    //       (y) => y == x.customerGroup\n    //     );\n    //     if (jaExiste.length > 0) return;\n    //     customers.vinculos.push(x.customerGroup);\n    //   });\n\n    //   naoContemGrupo.forEach((x) => {\n    //     customers.vinculos.push(x.customerName);\n    //   });\n    //   customers = customers.vinculos.join(\", \");\n    //   return customers;\n\n    // },\n    buildRows(data) {\n      let dataRows = [];\n\n      data.forEach((e) => {\n        // var vinculos = this.setGroups(e.customers);\n        dataRows.push({\n          col1: e.id,\n          col2: e.name + \" \" + e.surname,\n          col3: e.emailAddress,\n          col4: e.userProfile,\n          // col5: vinculos,\n          col6: {\n            text: e.isActive === true ? \"Ativo\" : \"Inativo\",\n            color: e.isActive === true ? \"success\" : \"grey\",\n          },\n        });\n      });\n\n      this.rows = dataRows;\n    },\n  },\n\n  mixins: [\n    validationRules,\n    getRouterData,\n    dataTable({ store: \"ModuleUsuarios\" }),\n    crudController({ store: \"ModuleUsuarios\" }),\n  ],\n  components: { TopSection, ActionsTable },\n};\n</script>\n\n<style scoped>\n.testeClass {\n  text-overflow: ellipsis !important;\n  overflow: hidden;\n  white-space: nowrap;\n}\n</style>\n"]}]}