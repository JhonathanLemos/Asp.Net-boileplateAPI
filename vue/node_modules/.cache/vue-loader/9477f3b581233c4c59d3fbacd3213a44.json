  </v-btn>\r\n          </div>\r\n        </div>\r\n      </v-form>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\nimport crudController from \"@/mixins/crudController.js\";\r\nimport enumPaises from \"@/lib/enumPaises.js\";\r\nimport validationRules from \"../../mixins/validationRules\";\r\nimport { mask } from \"vue-the-mask\";\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nimport CategoryBooleanChip from \"@/components/CategoryBooleanChip.vue\";\r\nimport DialogDelete from \"@/components/DialogDelete.vue\";\r\n\r\nconst modulePortos = createNamespacedHelpers(\"ModulePortos\");\r\n\r\nexport default {\r\n  directives: { mask },\r\n\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"id\" },\r\n      { text: \"Nome\", value: \"abbreviationConcatName\" },\r\n      { text: \"PaÃ­s\", value: \"country\" },\r\n      { text: \"Latitude\", value: \"latitude\" },\r\n      { text: \"Longitude\", value: \"longitude\" },\r\n      { text: \"Status\", value: \"status\" },\r\n    ],\r\n    rows: [],\r\n    showButtonInactive: false,\r\n    currentePort: {},\r\n    openDynamicDialogForDelete: [],\r\n    messageDialog: \"\",\r\n    enableBtnDelete: false,\r\n    loadingProgressBar: false,\r\n    loadingButton: false,\r\n  }),\r\n\r\n  computed: {\r\n    loadPaises() {\r\n      let paises = [];\r\n      Object.values(enumPaises).forEach((x) => {\r\n        paises.push({ value: x, text: x });\r\n      });\r\n      return paises;\r\n    },\r\n    \r\n    editPermission() {\r\n      return this.hasPermission(this.$PermissionNames.permissionPortEdit);\r\n    },\r\n  },\r\n  methods: {\r\n    ...modulePortos.mapActions([\"delete\", \"update\"]),\r\n\r\n    buildRows(data) {\r\n      this.rows = data;\r\n    },\r\n\r\n    async validatePortOnDelete(item) {\r\n      this.currentePort = item;\r\n      this.enableBtnDelete = false;\r\n      this.loadingProgressBar = true;\r\n      this.openDynamicDialogForDelete = [true];\r\n      this.messageDialog = \"\";\r\n\r\n      var result = await this.delete(item.id);\r\n\r\n      if (result.status == 200) {\r\n        this.enableBtnDelete = true;\r\n        this.showButtonInactive = false;\r\n        this.messageDialog = \"Deseja realmente excluir este Porto?\";\r\n      } else if (result.result == null || result.status == 400) {\r\n        this.loadMessageForItensWithDependency(item, result);\r\n      }\r\n\r\n      this.loadingProgressBar = false;\r\n    },\r\n\r\n    loadMessageForItensWithDependency(item, result) {\r\n      if (result.error.code != 547) {\r\n        this.loadMessageIfError(result);\r\n        return;\r\n      }\r\n\r\n      if (item.status) {\r\n        this.showButtonInactive = true;\r\n        this.messageDialog =\r\n          \"Existem registros vinculados a esse Porto e por isso ele nÃ£o pode ser excluÃ­do, somente inativado. Deseja inativar o Porto?\";\r\n      } else {\r\n        this.showButtonInactive = false;\r\n        this.messageDialog =\r\n          \"Existem registros vinculados a esse Porto e por isso ele nÃ£o pode ser excluÃ­do!\";\r\n      }\r\n    },\r\n\r\n    loadMessageIfError(result) {\r\n      this.messageDialog = result.error.message;\r\n      this.showButtonInactive = false;\r\n    },\r\n\r\n    async inactivatedPort() {\r\n      this.currentePort.status = false;\r\n      this.loadingButton = true;\r\n      await this.update(this.currentePort);\r\n      this.loadingButton = false;\r\n      this.loadEditionData = false;\r\n\r\n      this.showAlertSuccess(\"Registro inativado com sucesso!\");\r\n    },\r\n\r\n    async deletePort() {\r\n      this.loadingButton = true;\r\n      var result = await this.delete(this.currentePort.id);\r\n      this.loadingButton = false;\r\n\r\n      if (result.status == 200) {\r\n        this.showAlertSuccess(\"Registro excluido com sucesso!\");\r\n        this.key++;\r\n      }\r\n    },\r\n\r\n    showAlertSuccess(message) {\r\n      this.alertSuccess = true;\r\n      this.alertSuccessText = message;\r\n      setTimeout(() => {\r\n        this.alertSuccess = false;\r\n      }, 3000);\r\n    },\r\n\r\n    loadEditionData(data) {\r\n      this.dataSelectedItem = data;\r\n    },\r\n  },\r\n  mixins: [\r\n    validationRules,\r\n    getRouterData,\r\n    dataTable({ store: \"ModulePortos\" }),\r\n    crudController({ store: \"ModulePortos\" }),\r\n  ],\r\n\r\n  components: { TopSection, ActionsTable, CategoryBooleanChip, DialogDelete },\r\n};\r\n</script>"]}]}