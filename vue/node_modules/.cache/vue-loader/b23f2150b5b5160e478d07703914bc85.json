RouterData\"\r\n      labelSearch=\"Pesquisar por Código ou Razão Social\"\r\n    ></actions-table>\r\n\r\n    <div\r\n      style=\"overflow-y: auto\"\r\n      class=\"flex-grow-1 rounded-lg pa-4 white elevation-5\"\r\n    >\r\n      <!-- DATA TABLE -->\r\n      <v-data-table\r\n        :key=\"key\"\r\n        disable-sort\r\n        :headers=\"headers\"\r\n        :items=\"rows\"\r\n        :options.sync=\"options\"\r\n        :page=\"page\"\r\n        :server-items-length=\"totalItems\"\r\n        noDataText=\"Nenhum registro encontrado\"\r\n        :loading=\"tableLoading\"\r\n        loading-text=\"Aguarde, carregando clientes...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\"\r\n      >\r\n        <!-- Icone Manutenção -->\r\n        <template v-slot:[`item.col1`]=\"{ item }\">\r\n          <v-btn v-if=\"editPermission\" icon @click=\"updateItem(1, item.col1)\">\r\n            <v-icon> mdi-pencil </v-icon>\r\n          </v-btn>\r\n          <v-btn v-else icon @click=\"updateItem(1, item.col1)\">\r\n            <v-icon> mdi-eye </v-icon>\r\n          </v-btn>\r\n        </template>\r\n        <!-- Pendencias -->\r\n        <template v-slot:[`item.col2`]=\"{ item }\">\r\n          <v-icon color=\"orange\" v-if=\"item.col2 === 0\"> mdi-alert </v-icon>\r\n        </template>\r\n\r\n        <!-- Status -->\r\n        <template v-slot:[`item.col9`]=\"{ item }\">\r\n          <v-chip\r\n            :color=\"item.col9.color\"\r\n            outlined\r\n            class=\"mt-1\"\r\n            style=\"font-weight: 500\"\r\n          >\r\n            {{ item.col9.text | AtivoInativo }}\r\n          </v-chip>\r\n        </template>\r\n\r\n        <!-- Phone -->\r\n        <template v-slot:[`item.col7`]=\"{ item }\">\r\n          {{ item.col7.text | Telefone }}\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n\r\n    <!-- MODAL DE EDIÇÃO -->\r\n    <v-dialog persistent v-model=\"modalEdit\" max-width=\"1200px\">\r\n      <v-form ref=\"form\" v-model=\"valid\">\r\n        <div class=\"white pa-8 d-flex flex-column\">\r\n          <div\r\n            class=\"d-flex justify-space-between\"\r\n            style=\"border-bottom: 1px solid #e1e1e1\"\r\n          >\r\n            <h2 class=\"asia-neutral--text text--var-1 pb-2\">Editar Cliente</h2>\r\n\r\n            <v-btn icon @click=\"closeModal()\">\r\n              <v-icon>mdi-close</v-icon>\r\n            </v-btn>\r\n          </div>\r\n\r\n          <v-alert\r\n            type=\"error\"\r\n            dense\r\n            class=\"mt-1\"\r\n            outlined\r\n            v-show=\"errorAlert\"\r\n            transition=\"fade-transition\"\r\n            >{{ errorAlertText }}</v-alert\r\n          >\r\n\r\n          <div class=\"d-flex flex-row mt-4\">\r\n            <div v-if=\"editPermission\">\r\n              <v-switch\r\n                class=\"mr-10\"\r\n                v-model=\"dataSelectedItem.status\"\r\n                :label=\"dataSelectedItem.status == true ? 'Ativo' : 'Inativo'\"\r\n                :loading=\"fieldLoading\"\r\n              ></v-switch>\r\n            </div>\r\n            <div v-else>\r\n              <v-chip\r\n                class=\"mt-2 mr-10\"\r\n                outlined\r\n                :color=\"dataSelectedItem.status ? 'green' : 'red'\"\r\n              >\r\n                {{ dataSelectedItem.status == true ? \"Ativo\" : \"Inativo\" }}\r\n              </v-chip>\r\n            </div>\r\n\r\n            <v-text-field\r\n              v-model=\"dataSelectedItem.code\"\r\n              label=\"Código\"\r\n              style=\"width: 5%\"\r\n              readonly\r\n              disabled\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"dataSelectedItem.corporateName\"\r\n              label=\"Razão Social\"\r\n              class=\"ml-5\"\r\n              style=\"width: 40%\"\r\n              readonly\r\n              disabled\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"dataSelectedItem.cnpj\"\r\n              label=\"CNPJ\"\r\n              class=\"ml-5\"\r\n              readonly\r\n              disabled\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row mt-4\">\r\n            <v-text-field\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.plant\"\r\n              label=\"Planta *\"\r\n              validate-on-blur\r\n              style=\"width: 5%\"\r\n              :loading=\"fieldLoading\"\r\n              :rules=\"[rules.required]\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.responsible\"\r\n              label=\"Responsável\"\r\n              validate-on-blur\r\n              class=\"ml-5\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.email\"\r\n              label=\"E-mail (Opcional)\"\r\n              class=\"ml-5\"\r\n              :rules=\"[rules.emailNotRequired]\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n\r\n            <v-autocomplete\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.addressId\"\r\n              :items=\"ddiData\"\r\n              :search-input.sync=\"searchData\"\r\n              flat\r\n              type=\"number\"\r\n              validate-on-blur\r\n              hide-no-data\r\n              hide-selected\r\n              item-text=\"ddi\"\r\n              item-value=\"id\"\r\n              label=\"DDI\"\r\n              hint=\"Digite 1 caractere\"\r\n              persistent-hint\r\n              class=\"ml-5\"\r\n              style=\"width: 2%\"\r\n              :loading=\"fieldLoading\"\r\n            />\r\n\r\n            <v-text-field\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.phone\"\r\n              hide-spin-buttons\r\n              label=\"Telefone\"\r\n              class=\"ml-5\"\r\n              validate-on-blur\r\n              v-mask=\"['(##) ####-####']\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row mt-4\">\r\n            <v-select\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.customerType\"\r\n              :items=\"enumTipoCliente\"\r\n              label=\"Tipo de Cliente\"\r\n              validate-on-blur\r\n              multiple\r\n              small-chips\r\n              style=\"width: 50%\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-select>\r\n\r\n            <v-select\r\n              :disabled=\"!editPermission\"\r\n              v-model=\"dataSelectedItem.materialType\"\r\n              class=\"ml-5\"\r\n              :items=\"enumTipoMaterial\"\r\n              validate-on-blur\r\n              label=\"Tipo de Materiais\"\r\n              multiple\r\n              small-chips\r\n              style=\"width: 50%\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-select>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-column mt-4\">\r\n            <p\r\n              class=\"mb-0 asia-neutral--text text--var-3\"\r\n              style=\"font-size: 12px\"\r\n            >\r\n              {{\r\n                dataSelectedItem.groupName\r\n                  ? \"Grupos de Clientes\"\r\n                  : \"Nenhum grupo vinculado\"\r\n              }}\r\n            </p>\r\n\r\n            <div>\r\n              <v-chip\r\n                v-if=\"dataSelectedItem.groupName\"\r\n                color=\"#666\"\r\n                class=\"mt-2 mr-2\"\r\n                outlined\r\n                small\r\n                pill\r\n                >{{ dataSelectedItem.groupName }}</v-chip\r\n              >\r\n            </div>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row justify-end pt-5\">\r\n            <v-btn\r\n              v-if=\"editPermission\"\r\n              rounded\r\n              large\r\n              class=\"asia-red white--text\"\r\n              @click=\"updateItem(2)\"\r\n              :loading=\"loadingBtnSave\"\r\n              >SALVAR</v-btn\r\n            >\r\n          </div>\r\n        </div>\r\n      </v-form>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport enumTiposMateriais from \"@/lib/enumTiposMateriais.js\";\r\nimport validationRules from \"@/mixins/validationRules.js\";\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\nimport crudController from \"@/mixins/crudController.js\";\r\nimport { mask } from \"vue-the-mask\";\r\nimport { createNamespacedHelpers } from \"vuex\";\r\n\r\nconst moduleAddress = createNamespacedHelpers(\"ModuleAddress\");\r\n\r\nexport default {\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"col1\", width: \"1%\", align: \"center\" },\r\n      { text: \"Pendências\", value: \"col2\", width: \"11%\", align: \"center\" },\r\n      { text: \"Código\", value: \"col3\", width: \"1%\", align: \"center\" },\r\n      { text: \"CNPJ\", value: \"col4\", width: \"15%\", align: \"center\" },\r\n      { text: \"Planta\", value: \"col5\", width: \"5%\", align: \"center\" },\r\n      { text: \"Razão Social\", value: \"col6\", width: \"30%\", align: \"left\" },\r\n      { text: \"Telefone\", value: \"col7\", width: \"15%\", align: \"center\" },\r\n      { text: \"E-mail\", value: \"col8\", width: \"15%\", align: \"center\" },\r\n      { text: \"Status\", value: \"col9\", width: \"1%\", align: \"center\" },\r\n    ],\r\n    rows: [],\r\n    ddiData: [],\r\n    input: null,\r\n  }),\r\n\r\n  computed: {\r\n    enumTipoCliente() {\r\n      return this.$store.state.ModuleClientes.enumTipoCliente;\r\n    },\r\n    enumTipoMaterial() {\r\n      return this.$store.state.ModuleClientes.enumTipoMaterial;\r\n    },\r\n    editPermission() {\r\n      return this.hasPermission(this.$PermissionNames.permissionCustomerEdit);\r\n    },\r\n\r\n    searchData: {\r\n      get() {\r\n        return this.input;\r\n      },\r\n      set(newValue) {\r\n        this.input = newValue ? newValue.toString() : newValue;\r\n      },\r\n    },\r\n    loadMaterialTypes() {\r\n      let materialTypes = [];\r\n      Object.values(enumTiposMateriais).forEach((x, i) => {\r\n        materialTypes.push({ value: i + 1, text: x });\r\n      });\r\n      return materialTypes;\r\n    },\r\n  },\r\n  methods: {\r\n    ...moduleAddress.mapActions({\r\n      GetFilteredData: \"GetFilteredData\",\r\n    }),\r\n\r\n    buildRows(data) {\r\n      this.rows = data.map((e) => ({\r\n        col1: e.id,\r\n        col2: e.lastModificationTime !== null ? 1 : 0,\r\n        col3: e.code,\r\n        col4: e.cnpj,\r\n        col5: e.plant,\r\n        col6: e.corporateName,\r\n        col7: {\r\n          text: \"+\" + e.address + \" \" + e.phone,\r\n        },\r\n        col8: e.email,\r\n        col9: {\r\n          text: e.status,\r\n          color: e.status === true ? \"success\" : \"grey\",\r\n        },\r\n      }));\r\n    },\r\n\r\n    loadEditionData(data) {\r\n      this.dataSelectedItem = data;\r\n      if (this.dataSelectedItem.address)\r\n        this.ddiData = [this.dataSelectedItem.address];\r\n    },\r\n\r\n    async loadPhoneData(v) {\r\n      this.GetFilteredData(v)\r\n        .then((response) => {\r\n          this.ddiData = response.data.result;\r\n        })\r\n        .catch(() => {\r\n          this.ddiData = [];\r\n        });\r\n    },\r\n  },\r\n\r\n  watch: {\r\n    searchData(val) {\r\n      val &&\r\n        (!this.dataSelectedItem.address ||\r\n          parseInt(val) !== this.dataSelectedItem.address.ddi) &&\r\n        this.loadPhoneData(val);\r\n    },\r\n  },\r\n\r\n  directives: { mask },\r\n\r\n  mixins: [\r\n    validationRules,\r\n    getRouterData,\r\n    dataTable({ store: \"ModuleClientes\" }),\r\n    crudController({ store: \"ModuleClientes\" }),\r\n  ],\r\n  components: { TopSection, ActionsTable },\r\n};\r\n</script>\r\n\r\n<style>\r\n.required label::after {\r\n  content: \"*\";\r\n}\r\n</style>\r\n"]}]}