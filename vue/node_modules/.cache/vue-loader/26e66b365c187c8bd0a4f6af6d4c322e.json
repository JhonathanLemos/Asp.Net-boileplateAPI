  class=\"d-flex flex-column justify-space-between mx-10\"\r\n    style=\"width: 100%; height: 98vh\"\r\n  >\r\n    <top-section></top-section>\r\n\r\n    <actions-table\r\n      :params=\"getRouterData\"\r\n      :disableButton=\"!editPermission\"\r\n      v-on:clickAdd=\"addNewItem(1)\"\r\n      labelSearch=\"Pesquisar por Utilização ou Descrição\"\r\n    ></actions-table>\r\n\r\n    <v-alert\r\n      type=\"success\"\r\n      transition=\"slide-y-transition\"\r\n      dismissible\r\n      v-show=\"alertSuccess\"\r\n      dense\r\n      >{{ alertSuccessText }}</v-alert\r\n    >\r\n\r\n    <v-alert\r\n      type=\"error\"\r\n      dense\r\n      class=\"mt-1\"\r\n      outlined\r\n      v-show=\"alertError\"\r\n      transition=\"fade-transition\"\r\n      >{{ alertErrorText }}</v-alert\r\n    >\r\n\r\n    <div\r\n      style=\"overflow-y: auto\"\r\n      class=\"flex-grow-1 rounded-lg pa-4 white elevation-5\"\r\n    >\r\n      <!-- DATA TABLE -->\r\n      <v-data-table\r\n        :key=\"key\"\r\n        disable-sort\r\n        :headers=\"\r\n          editPermission ? headers : headers.filter((x) => x.value != 'id')\r\n        \"\r\n        :items=\"rows\"\r\n        :options.sync=\"options\"\r\n        :page=\"page\"\r\n        :server-items-length=\"totalItems\"\r\n        noDataText=\"Nenhum registro encontrado\"\r\n        :loading=\"tableLoading\"\r\n        loading-text=\"Aguarde, carregando categorias...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\"\r\n      >\r\n        <!-- Icone Manutenção -->\r\n        <template v-slot:[`item.id`]=\"{ item }\">\r\n          <v-btn icon @click=\"updateItem(1, item.id)\">\r\n            <v-icon> mdi-pencil </v-icon>\r\n          </v-btn>\r\n          <v-btn icon @click=\"validateCategoryStatusOnDelete(item)\">\r\n            <v-icon> mdi-delete </v-icon>\r\n          </v-btn>\r\n        </template>\r\n\r\n        <template v-slot:[`item.categoryStatusType`]=\"{ item }\">\r\n          {{ item.categoryStatusType | enumTiposCategoriasOptionsConvert }}\r\n        </template>\r\n\r\n        <template v-slot:[`item.flag`]=\"{ item }\">\r\n          {{ item.flag ? \"SIM\" : \"NÃO\" }}\r\n        </template>\r\n\r\n        <!-- Color -->\r\n        <template v-slot:[`item.color`]=\"{ item }\">\r\n          <v-avatar\r\n            class=\"elevation-2\"\r\n            size=\"30px\"\r\n            :color=\"item.color\"\r\n          ></v-avatar>\r\n        </template>\r\n\r\n        <!-- Icone -->\r\n        <template v-slot:[`item.icon`]=\"{ item }\">\r\n          <v-icon>mdi-{{ item.icon }}</v-icon>\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n\r\n    <dialog-delete\r\n      :message=\"messageDialog\"\r\n      :showButtonInactive=\"false\"\r\n      :loadingButton=\"loadingButton\"\r\n      :showBtnDelete=\"enableBtnDelete\"\r\n      v-model=\"openDynamicDialogForDelete\"\r\n      :loading=\"loadingProgressBar\"\r\n      @excluir=\"deleteCategoryStatus()\"\r\n    />\r\n\r\n    <!-- MODAL DE ADIÇÃO -->\r\n    <v-dialog persistent v-model=\"modalAdd\" max-width=\"1000px\">\r\n      <v-form ref=\"form\" v-model=\"valid\">\r\n        <div class=\"white pa-8 d-flex flex-column\">\r\n          <div\r\n            class=\"d-flex flex-row justify-space-between\"\r\n            style=\"border-bottom: 1px solid #e1e1e1\"\r\n          >\r\n            <h2>Cadastrar Categoria/Situação</h2>\r\n            <v-btn @click=\"closeModal()\" text small fab>\r\n              <v-icon>mdi-close</v-icon>\r\n            </v-btn>\r\n          </div>\r\n          <v-alert\r\n            type=\"error\"\r\n            dense\r\n            class=\"mt-1\"\r\n            outlined\r\n            v-show=\"errorAlert\"\r\n            transition=\"fade-transition\"\r\n            >{{ errorAlertText }}</v-alert\r\n          >\r\n\r\n          <div class=\"d-flex flex-row mt-4\">\r\n            <v-autocomplete\r\n              :items=\"loadCategoryTypes\"\r\n              clearable\r\n              label=\"Utilização\"\r\n              item-color=\"primary\"\r\n              :rules=\"[rules.required]\"\r\n              v-model=\"dataNewItem.categoryStatusType\"\r\n              no-data-text=\"Nenhum dado encontrado\"\r\n            ></v-autocomplete>\r\n          </div>\r\n\r\n          <v-text-field\r\n            v-model=\"dataNewItem.description\"\r\n            label=\"Descrição Pt\"\r\n            counter=\"20\"\r\n            :rules=\"[\r\n              rules.required,\r\n              () =>\r\n                (dataNewItem.description &&\r\n                  dataNewItem.description.length <= 20) ||\r\n                'Máximo de 20 caracteres',\r\n            ]\"\r\n          ></v-text-field>\r\n\r\n          <v-text-field\r\n            v-model=\"dataNewItem.descriptionEn\"\r\n            label=\"Descrição En\"\r\n            counter=\"20\"\r\n            :rules=\"[\r\n              rules.required,\r\n              () =>\r\n                (dataNewItem.descriptionEn &&\r\n                  dataNewItem.descriptionEn.length <= 20) ||\r\n                'Máximo de 20 caracteres',\r\n            ]\"\r\n          ></v-text-field>\r\n\r\n          <div class=\"d-flex flex-row mb-4 align-center\">\r\n            <span class=\"asia-neutral--text text--var-3 mr-3\">Ícone</span>\r\n            <v-btn-toggle v-model=\"dataNewItem.icon\" mandatory class=\"mr-2\">\r\n              <v-btn value=\"check-circle\">\r\n                <v-icon>mdi-check</v-icon>\r\n              </v-btn>\r\n\r\n              <v-btn value=\"alert-outline\">\r\n                <v-icon>mdi-alert-outline</v-icon>\r\n              </v-btn>\r\n\r\n              <v-btn value=\"arrow-right-thin-circle-outline\">\r\n                <v-icon>mdi-arrow-right-thin-circle-outline</v-icon>\r\n              </v-btn>\r\n\r\n              <v-btn value=\"pause-circle-outline\">\r\n                <v-icon>mdi-pause-circle-outline</v-icon>\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n\r\n            <div style=\"width: 10%\" />\r\n\r\n            <span class=\"asia-neutral--text text--var-3 mr-5\">Cor</span>\r\n            <v-btn-toggle\r\n              class=\"mr-10 elevation-2\"\r\n              mandatory\r\n              v-model=\"dataNewItem.color\"\r\n            >\r\n              <v-btn value=\"#1FF479\" depressed color=\"#1FF479\">\r\n                <v-icon v-if=\"dataNewItem.color == '#1FF479'\">mdi-check</v-icon>\r\n              </v-btn>\r\n              <v-btn value=\"#A00C0F\" depressed color=\"#A00C0F\">\r\n                <v-icon v-if=\"dataNewItem.color == '#A00C0F'\" color=\"white\"\r\n                  >mdi-check</v-icon\r\n                >\r\n              </v-btn>\r\n\r\n              <v-btn value=\"#FECC00\" depressed color=\"#FECC00\">\r\n                <v-icon v-if=\"dataNewItem.color == '#FECC00'\">mdi-check</v-icon>\r\n              </v-btn>\r\n              <v-btn value=\"#FFFFFF\" depressed color=\"#FFFFFF\">\r\n                <v-icon v-if=\"dataNewItem.color == '#FFFFFF'\">mdi-check</v-icon>\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n\r\n            <div style=\"width: 5%\" />\r\n\r\n            <v-checkbox\r\n              label=\"Categoria/Situação Conclusiva\"\r\n              v-model=\"dataNewItem.flag\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-checkbox>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row justify-end pt-5\">\r\n            <v-btn rounded large text class=\"mr-4\" @click=\"closeModal()\"\r\n              >CANCELAR</v-btn\r\n            >\r\n            <v-btn\r\n              rounded\r\n              large\r\n              class=\"asia-red white--text\"\r\n              @click=\"addNewItem(2)\"\r\n              :loading=\"loadingBtnSave\"\r\n              >SALVAR</v-btn\r\n            >\r\n          </div>\r\n        </div>\r\n      </v-form>\r\n    </v-dialog>\r\n\r\n    <!-- MODAL DE EDIÇÃO -->\r\n    <v-dialog persistent v-model=\"modalEdit\" max-width=\"1000px\">\r\n      <v-form ref=\"form\" v-model=\"valid\">\r\n        <div class=\"white pa-8 d-flex flex-column\">\r\n          <div\r\n            class=\"d-flex flex-row justify-space-between\"\r\n            style=\"border-bottom: 1px solid #e1e1e1\"\r\n          >\r\n            <h2>Cadastrar Categoria/Situação</h2>\r\n            <v-btn @click=\"closeModal()\" text small fab>\r\n              <v-icon>mdi-close</v-icon>\r\n            </v-btn>\r\n          </div>\r\n\r\n          <v-alert\r\n            type=\"error\"\r\n            dense\r\n            class=\"mt-1\"\r\n            outlined\r\n            v-show=\"errorAlert\"\r\n            transition=\"fade-transition\"\r\n            >{{ errorAlertText }}</v-alert\r\n          >\r\n\r\n          <div class=\"d-flex flex-row mt-4\">\r\n            <v-autocomplete\r\n              :items=\"loadCategoryTypes\"\r\n              clearable\r\n              label=\"Utilização\"\r\n              :rules=\"[rules.required]\"\r\n              validate-on-blur\r\n              v-model=\"dataSelectedItem.categoryStatusType\"\r\n              :loading=\"fieldLoading\"\r\n              no-data-text=\"Nenhum dado encontrado\"\r\n            ></v-autocomplete>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row mt-4\">\r\n            <v-text-field\r\n              v-model=\"dataSelectedItem.description\"\r\n              label=\"Descrição Pt\"\r\n              :loading=\"fieldLoading\"\r\n              :rules=\"[rules.required]\"\r\n              validate-on-blur\r\n              class=\"mr-4\"\r\n            ></v-text-field>\r\n\r\n            <v-text-field\r\n              v-model=\"dataSelectedItem.descriptionEn\"\r\n              label=\"Descrição En\"\r\n              :rules=\"[rules.required]\"\r\n              validate-on-blur\r\n              :loading=\"fieldLoading\"\r\n            ></v-text-field>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row mb-4 align-center mt-4\">\r\n            <span style=\"color: #999\">Ícone</span>\r\n            <v-btn-toggle\r\n              mandatory\r\n              v-model=\"dataSelectedItem.icon\"\r\n              class=\"ml-5 mr-12\"\r\n            >\r\n              <v-btn value=\"check-circle\">\r\n                <v-icon>mdi-check</v-icon>\r\n              </v-btn>\r\n\r\n              <v-btn value=\"alert-outline\">\r\n                <v-icon>mdi-alert-outline</v-icon>\r\n              </v-btn>\r\n\r\n              <v-btn value=\"arrow-right-thin-circle-outline\">\r\n                <v-icon>mdi-arrow-right-thin-circle-outline</v-icon>\r\n              </v-btn>\r\n\r\n              <v-btn value=\"pause-circle-outline\">\r\n                <v-icon>mdi-pause-circle-outline</v-icon>\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n\r\n            <span class=\"asia-neutral--text text--var-3 mr-5\">Cor</span>\r\n            <v-btn-toggle\r\n              class=\"mr-10 elevation-2\"\r\n              v-model=\"dataSelectedItem.color\"\r\n              mandatory\r\n            >\r\n              <v-btn value=\"#1FF479\" depressed color=\"#1FF479\">\r\n                <v-icon v-if=\"dataSelectedItem.color == '#1FF479'\"\r\n                  >mdi-check</v-icon\r\n                >\r\n              </v-btn>\r\n              <v-btn value=\"#A00C0F\" depressed color=\"#A00C0F\">\r\n                <v-icon v-if=\"dataSelectedItem.color == '#A00C0F'\" color=\"white\"\r\n                  >mdi-check</v-icon\r\n                >\r\n              </v-btn>\r\n\r\n              <v-btn value=\"#FECC00\" depressed color=\"#FECC00\">\r\n                <v-icon v-if=\"dataSelectedItem.color == '#FECC00'\"\r\n                  >mdi-check</v-icon\r\n                >\r\n              </v-btn>\r\n              <v-btn value=\"#FFFFFF\" depressed color=\"#FFFFFF\">\r\n                <v-icon v-if=\"dataSelectedItem.color == '#FFFFFF'\"\r\n                  >mdi-check</v-icon\r\n                >\r\n              </v-btn>\r\n            </v-btn-toggle>\r\n\r\n            <v-checkbox\r\n              label=\"Categoria/Situação Conclusiva\"\r\n              class=\"ml-10\"\r\n              v-model=\"dataSelectedItem.flag\"\r\n              :loading=\"fieldLoading\"\r\n            ></v-checkbox>\r\n          </div>\r\n\r\n          <div class=\"d-flex flex-row justify-end pt-5\">\r\n            <v-btn rounded large text class=\"mr-4\" @click=\"closeModal()\"\r\n              >CANCELAR</v-btn\r\n            >\r\n            <v-btn\r\n              rounded\r\n              large\r\n              class=\"asia-red white--text\"\r\n              @click=\"updateItem(2)\"\r\n              :loading=\"loadingBtnSave\"\r\n              >SALVAR</v-btn\r\n            >\r\n          </div>\r\n        </div>\r\n      </v-form>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\nimport crudController from \"@/mixins/crudController.js\";\r\nimport tiposCategorias from \"@/lib/enumTiposCategorias.js\";\r\nimport validationRules from \"../../mixins/validationRules\";\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nimport DialogDelete from \"@/components/DialogDelete.vue\";\r\n\r\nconst moduleCategoriaSituacao = createNamespacedHelpers(\r\n  \"ModuleCategoriaSituacao\"\r\n);\r\n\r\nexport default {\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"id\", width: \"10%\" },\r\n      { text: \"Utilização\", value: \"categoryStatusType\", width: \"20%\" },\r\n      { text: \"Descrição\", value: \"description\", width: \"20%\" },\r\n      { text: \"Cor\", value: \"color\", width: \"\" },\r\n      { text: \"Ícone\", value: \"icon\", align: \"center\", width: \"30%\" },\r\n      { text: \"Situação Conclusiva\", value: \"flag\", width: \"\" },\r\n    ],\r\n    rows: [],\r\n    modalAdd: false,\r\n    modalEdit: false,\r\n    currentCategoryStatus: {},\r\n    openDynamicDialogForDelete: [],\r\n    messageDialog: \"\",\r\n    enableBtnDelete: false,\r\n    loadingProgressBar: false,\r\n    loadingButton: false,\r\n    alertError: false,\r\n    alertErrorText: \"\",\r\n  }),\r\n\r\n  computed: {\r\n    loadCategoryTypes() {\r\n      let categoryTypes = [];\r\n      Object.values(tiposCategorias.enumTiposCategoriasOptions).forEach(\r\n        (x, i) => {\r\n          categoryTypes.push({ value: i + 1, text: x });\r\n        }\r\n      );\r\n      return categoryTypes;\r\n    },\r\n    editPermission() {\r\n      return this.hasPermission(\r\n        this.$PermissionNames.permissionCategorySituationEdit\r\n      );\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    ...moduleCategoriaSituacao.mapActions([\"delete\", \"update\"]),\r\n\r\n    buildRows(data) {\r\n      this.rows = data;\r\n    },\r\n    loadEditionData(data) {\r\n      this.dataSelectedItem = data;\r\n    },\r\n\r\n    async validateCategoryStatusOnDelete(item) {\r\n      this.currentCategoryStatus = item;\r\n      this.enableBtnDelete = false;\r\n      this.loadingProgressBar = true;\r\n      this.openDynamicDialogForDelete = [true];\r\n      this.messageDialog = \"\";\r\n\r\n      var result = await this.delete(item.id);\r\n\r\n      if (result.status == 200) {\r\n        this.enableBtnDelete = true;\r\n        this.messageDialog =\r\n          \"Deseja realmente excluir esta Categoria/Situação?\";\r\n      } else if (result.result == null || result.status == 400) {\r\n        this.loadMessageForItensWithDependency(item, result);\r\n      }\r\n\r\n      this.loadingProgressBar = false;\r\n    },\r\n\r\n    loadMessageForItensWithDependency(result) {\r\n      if (result.error.code != 547) {\r\n        this.loadMessageIfError(result);\r\n        return;\r\n      }\r\n\r\n      this.messageDialog =\r\n        \"Existem registros vinculados a essa Categoria/Situação e por isso ele não pode ser excluído!\";\r\n    },\r\n\r\n    loadMessageIfError(result) {\r\n      this.messageDialog = result.error.message;\r\n    },\r\n\r\n    async deleteCategoryStatus() {\r\n      this.loadingButton = true;\r\n      var result = await this.delete(this.currentCategoryStatus.id);\r\n      this.loadingButton = false;\r\n\r\n      if (result.status == 200) {\r\n        this.showAlertSuccess(\"Registro excluido com sucesso!\");\r\n        this.key++;\r\n      } else {\r\n        this.showAlertError(result.error.message);\r\n      }\r\n    },\r\n\r\n    showAlertError(message) {\r\n      this.alertError = true;\r\n      this.alertErrorText = message;\r\n      setTimeout(() => {\r\n        this.alertError = false;\r\n      }, 3000);\r\n    },\r\n\r\n    showAlertSuccess(message) {\r\n      this.alertSuccess = true;\r\n      this.alertSuccessText = message;\r\n      setTimeout(() => {\r\n        this.alertSuccess = false;\r\n      }, 3000);\r\n    },\r\n  },\r\n\r\n  mixins: [\r\n    validationRules,\r\n    getRouterData,\r\n    dataTable({ store: \"ModuleCategoriaSituacao\" }),\r\n    crudController({ store: \"ModuleCategoriaSituacao\" }),\r\n  ],\r\n  components: { TopSection, ActionsTable, DialogDelete },\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"]}]}