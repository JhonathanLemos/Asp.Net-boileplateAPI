{"remainingRequest":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\src\\views\\ncm\\ViewNCM.vue?vue&type=script&lang=js&","dependencies":[{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\src\\views\\ncm\\ViewNCM.vue","mtime":1680609356647},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1683229777571},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1683229780390}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ViewNCM.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0HA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"ViewNCM.vue","sourceRoot":"src/views/ncm","sourcesContent":["<template>\r\n  <div\r\n    class=\"d-flex flex-column justify-space-between mx-10\"\r\n    style=\"width: 100%; height: 98vh\"\r\n  >\r\n    <top-section></top-section>\r\n\r\n    <actions-table\r\n      :params=\"getRouterData\"\r\n      :disableButton=\"!editPermission\"\r\n      v-on:clickAdd=\"$router.push(`ncm/add`)\"\r\n      labelSearch=\"Pesquisar por NCM, Categoria-Material ou Descrição\"\r\n    ></actions-table>\r\n\r\n    <v-alert\r\n      type=\"success\"\r\n      transition=\"slide-y-transition\"\r\n      dismissible\r\n      v-show=\"alertSuccess\"\r\n      dense\r\n      >{{ alertSuccessText }}</v-alert\r\n    >\r\n\r\n    <div\r\n      style=\"overflow-y: auto\"\r\n      class=\"flex-grow-1 rounded-lg pa-4 white elevation-5\"\r\n    >\r\n      <v-data-table\r\n        :key=\"key\"\r\n        disable-sort\r\n        :headers=\"\r\n          editPermission ? headers : headers.filter((x) => x.value != 'col1')\r\n        \"\r\n        :items=\"rows\"\r\n        :options.sync=\"options\"\r\n        :page=\"page\"\r\n        :server-items-length=\"totalItems\"\r\n        noDataText=\"Nenhum registro encontrado\"\r\n        :loading=\"tableLoading\"\r\n        loading-text=\"Aguarde, carregando NCMs...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\">\r\n        <!-- Icone Manutenção -->\r\n        <template v-slot:[`item.id`]=\"{ item }\">\r\n          <div v-if=\"editPermission\" class=\"d-flex flex-nowrap\">\r\n            <v-btn icon @click=\"$router.push(`ncm/edit/${item.id}`)\">\r\n              <v-icon> mdi-pencil </v-icon>\r\n            </v-btn>\r\n\r\n            <v-btn icon @click=\"deleteItem(1, item.id)\">\r\n              <v-icon> mdi-delete </v-icon>\r\n            </v-btn>\r\n          </div>\r\n          <div v-else class=\"d-flex flex-nowrap\">\r\n            <v-btn icon @click=\"$router.push(`ncm/edit/${item.id}`)\">\r\n              <v-icon> mdi-eye </v-icon>\r\n            </v-btn>\r\n          </div>\r\n        </template>\r\n\r\n        <template v-slot:[`item.materialCategory`]=\"{ item }\">\r\n          {{\r\n            item.materialCategory.description +\r\n            \" - \" +\r\n            item.materialCategory.material\r\n          }}\r\n        </template>\r\n\r\n        <template v-slot:[`item.description`]=\"{ item }\">\r\n          <div class=\"text-truncate\" style=\"max-width: 170px\">\r\n            {{ item.description }}\r\n          </div>\r\n        </template>\r\n\r\n        <!-- Status -->\r\n        <template v-slot:[`item.status`]=\"{ item }\">\r\n          <v-chip :color=\"item.status.color\" outlined style=\"font-weight: 500\">\r\n            {{ item.status.text | AtivoInativo }}\r\n          </v-chip>\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n\r\n    <!-- DIALOG CONFIRM DELETE -->\r\n    <v-dialog v-model=\"dialogDeleteConfirm\" persistent max-width=\"450px\">\r\n      <div class=\"white pa-8 d-flex flex-column align-center text-center\">\r\n        <v-icon style=\"font-size: 60px\" color=\"asia-red\"\r\n          >mdi-alert-octagon</v-icon\r\n        >\r\n\r\n        <h4 class=\"asia-neutral--text text--var-1 pb-5 pt-5\">\r\n          Deseja excluir este NCM? <br />\r\n          Essa ação não pode ser desfeita.\r\n        </h4>\r\n\r\n        <div class=\"d-flex flex-row justify-end\">\r\n          <v-btn\r\n            rounded\r\n            large\r\n            text\r\n            class=\"mr-4\"\r\n            @click=\"dialogDeleteConfirm = false\"\r\n            >CANCELAR</v-btn\r\n          >\r\n\r\n          <v-btn\r\n            rounded\r\n            large\r\n            class=\"asia-red white--text\"\r\n            @click=\"deleteItem(2)\"\r\n            :loading=\"loadingBtnDelete\"\r\n            >EXCLUIR</v-btn\r\n          >\r\n        </div>\r\n      </div>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\nimport validationRules from \"../../mixins/validationRules\";\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nimport crudController from \"../../mixins/crudController\";\r\n\r\nconst moduleNCM = createNamespacedHelpers(\"ModuleNCM\");\r\n\r\nexport default {\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"id\", width: \"10%\" },\r\n      { text: \"NCM\", value: \"ncmCode\", width: \"20%\" },\r\n      { text: \"Categoria - Material\", value: \"materialCategory\", width: \"20%\" },\r\n      { text: \"Descrição\", value: \"description\", width: \"20%\" },\r\n      { text: \"Status\", value: \"status\", width: \"10%\" },\r\n    ],\r\n    rows: [],\r\n    key: null,\r\n    deleteEntity: null,\r\n    dialogDeleteConfirm: false,\r\n    alertSuccess: false,\r\n    alertSuccessText: null,\r\n  }),\r\n\r\n  computed: {\r\n    editPermission() {\r\n      return this.hasPermission(this.$PermissionNames.permissionNCMEdit);\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    ...moduleNCM.mapActions({ delete: \"delete\" }),\r\n\r\n    buildRows(data) {\r\n      let dataRows = [];\r\n      data.forEach((e) =>\r\n        dataRows.push({\r\n          id: e.id,\r\n          ncmCode: e.ncmCode,\r\n          materialCategory: e.materialCategory,\r\n          description: e.description,\r\n          status: {\r\n            text: e.status,\r\n            color: e.status === true ? \"success\" : \"grey\",\r\n          },\r\n        })\r\n      );\r\n      this.rows = dataRows;\r\n    },\r\n  },\r\n\r\n  mixins: [\r\n    validationRules,\r\n    getRouterData,\r\n    dataTable({ store: \"ModuleNCM\" }),\r\n    crudController({ store: \"ModuleNCM\" }),\r\n  ],\r\n  components: { TopSection, ActionsTable },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}