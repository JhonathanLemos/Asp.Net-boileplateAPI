\n    async loadDeliveryCategories() {\r\n      this.loadingDeliveryCategories = true;\r\n      this.deliveryCategories = await this.getByType(\r\n        tiposCategorias.enumTiposCategoriasRequest.situacaoEntrega\r\n      );\r\n      this.loadingDeliveryCategories = false;\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.hasPermission(this.$PermissionNames.permissionCategorySituationSee))\r\n      this.loadDeliveryCategories();\r\n  },\r\n};\r\n</script>"]}]}