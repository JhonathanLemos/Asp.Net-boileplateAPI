       label=\"Relatório de Inspeção\"\r\n        />\r\n      </v-col>\r\n    </v-row>\r\n  </div>\r\n</template>\r\n<script>\r\nimport tiposCategorias from \"@/lib/enumTiposCategorias.js\";\r\nimport DatePicker from \"@/components/DatePicker\";\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nconst moduleCategoriaSituacao = createNamespacedHelpers(\r\n  \"ModuleCategoriaSituacao\"\r\n);\r\nexport default {\r\n  props: [\"value\", \"readonly\"],\r\n  data: () => ({\r\n    productionCategories: [],\r\n    inspectionCategories: [],\r\n    loadingInspCategories: false,\r\n    loadingProdCategories: false,\r\n    dataFollowUp: {},\r\n  }),\r\n  computed: {\r\n    inspectionReportFile: {\r\n      get() {\r\n        return this.buildInspectionFile();\r\n      },\r\n      set(newValue) {\r\n        this.dataFollowUp.inspectionReportFile = newValue;\r\n        this.dataFollowUp.inspectionReportFileName = newValue\r\n          ? newValue.name\r\n          : null;\r\n      },\r\n    },\r\n  },\r\n  watch: {\r\n    value(newValue) {\r\n      this.dataFollowUp = newValue;\r\n    },\r\n  },\r\n  components: {\r\n    DatePicker,\r\n  },\r\n  methods: {\r\n    ...moduleCategoriaSituacao.mapActions([\"getByType\"]),\r\n    buildInspectionFile() {\r\n      if (\r\n        this.dataFollowUp.inspectionReportFile ||\r\n        this.dataFollowUp.inspectionReportFileName\r\n      ) {\r\n        return new File(\r\n          [this.dataFollowUp.inspectionReportFile],\r\n          this.dataFollowUp.inspectionReportFileName,\r\n          {\r\n            type: \"base64\",\r\n          }\r\n        );\r\n      }\r\n    },\r\n    async loadProductionCategories() {\r\n      this.loadingProdCategories = true;\r\n      this.productionCategories = await this.getByType(\r\n        tiposCategorias.enumTiposCategoriasRequest.situacaoProducao\r\n      );\r\n      this.loadingProdCategories = false;\r\n    },\r\n    async loadInspectionCategories() {\r\n      this.loadingInspCategories = true;\r\n      this.inspectionCategories = await this.getByType(\r\n        tiposCategorias.enumTiposCategoriasRequest.situaçãoInspecao\r\n      );\r\n      this.loadingInspCategories = false;\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.hasPermission(this.$PermissionNames.permissionCategorySituationSee)) {\r\n      this.loadProductionCategories();\r\n      this.loadInspectionCategories();\r\n    }\r\n  },\r\n};\r\n</script>\r\n"]}]}