{"remainingRequest":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\src\\views\\noticias\\ViewNoticiasCreate.vue?vue&type=style&index=0&lang=css&","dependencies":[{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\src\\views\\noticias\\ViewNoticiasCreate.vue","mtime":1680609356650},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\css-loader\\dist\\cjs.js","mtime":1683229778387},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":1683229781560},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\postcss-loader\\src\\index.js","mtime":1683229779724},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\cache-loader\\dist\\cjs.js","mtime":1683229777571},{"path":"C:\\Users\\lemos\\OneDrive\\Documentos\\My Web Sites\\7.3.0\\vue\\node_modules\\vue-loader\\lib\\index.js","mtime":1683229780390}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["ViewNoticiasCreate.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAmQA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA","file":"ViewNoticiasCreate.vue","sourceRoot":"src/views/noticias","sourcesContent":["<template>\r\n  <div\r\n    class=\"d-flex flex-column justify-space-between mx-10\"\r\n    style=\"width: 100%; height: 98vh\"\r\n  >\r\n    <top-section />\r\n\r\n    <v-alert\r\n      type=\"error\"\r\n      transition=\"slide-y-transition\"\r\n      dismissible\r\n      v-show=\"displayModalError\"\r\n      dense\r\n      >{{ displayModalErrorText }}</v-alert\r\n    >\r\n\r\n    <v-alert\r\n      type=\"success\"\r\n      transition=\"slide-y-transition\"\r\n      dismissible\r\n      v-show=\"alertSuccess\"\r\n      dense\r\n    >\r\n      {{ textSuccessAlert }}\r\n    </v-alert>\r\n\r\n    <div class=\"d-flex flex-row align-center mb-5\">\r\n      <div class=\"d-flex flex-row align-center\">\r\n        <h1 v-if=\"this.id\" class=\"asia-neutral--text text--var-1\">\r\n          Editar Notícia\r\n        </h1>\r\n        <h1 v-else class=\"asia-neutral--text text--var-1\">Criar Notícia</h1>\r\n      </div>\r\n\r\n      <div v-if=\"editPermission\" class=\"d-flex flex-row ml-4\">\r\n        <v-switch :label=\"status ? 'Ativo' : 'Inativo'\" v-model=\"status\" />\r\n      </div>\r\n      <div v-else class=\"mt-2 d-flex flex-row ml-4\">\r\n        <v-chip outlined :color=\"formData.status ? 'green' : 'red'\">\r\n          {{ formData.status == true ? \"Ativo\" : \"Inativo\" }}\r\n        </v-chip>\r\n      </div>\r\n    </div>\r\n\r\n    <div\r\n      style=\"overflow-y: auto\"\r\n      class=\"flex-grow-1 rounded-lg white elevation-3\"\r\n    >\r\n      <v-tabs background-color=\"#A00C0F\" dark flat v-model=\"tabs\">\r\n        <v-tab href=\"#tab-1\">Português</v-tab>\r\n        <v-tab href=\"#tab-2\">Inglês</v-tab>\r\n        <Loading\r\n          class=\"pa-2 align-self-center\"\r\n          v-if=\"loadingFields\"\r\n          :size=\"30\"\r\n          color=\"white\"\r\n        />\r\n      </v-tabs>\r\n\r\n      <v-form class=\"px-5 mb-2\" ref=\"form\">\r\n        <v-tabs-items v-model=\"tabs\">\r\n          <v-tab-item value=\"tab-1\">\r\n            <NewsLanguageTab\r\n              v-model=\"dataNews[0]\"\r\n              :readonly=\"!editPermission\"\r\n            />\r\n          </v-tab-item>\r\n          <v-tab-item value=\"tab-2\" eager>\r\n            <NewsLanguageTab\r\n              v-model=\"dataNews[1]\"\r\n              :readonly=\"!editPermission\"\r\n            />\r\n          </v-tab-item>\r\n        </v-tabs-items>\r\n      </v-form>\r\n    </div>\r\n\r\n    <div v-if=\"editPermission\" class=\"d-flex flex-row justify-end mt-3 mb-2\">\r\n      <v-btn\r\n        rounded\r\n        large\r\n        text\r\n        class=\"mr-3\"\r\n        @click=\"$router.push('/home/noticias')\"\r\n        >CANCELAR</v-btn\r\n      >\r\n\r\n      <v-btn\r\n        rounded\r\n        large\r\n        :loading=\"loadingBtnSave\"\r\n        class=\"asia-red white--text\"\r\n        @click=\"submit(2)\"\r\n        >SALVAR</v-btn\r\n      >\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport crudController from \"../../mixins/crudController\";\r\nimport NewsLanguageTab from \"./LanguageTabs/NewsLanguageTab.vue\";\r\nimport { createNamespacedHelpers } from \"vuex\";\r\nimport { toBase64 } from \"@/lib/helpers/base64\";\r\nimport Loading from \"../../components/Loading.vue\";\r\nconst moduleNoticias = createNamespacedHelpers(\"ModuleNoticias\");\r\n\r\nexport default {\r\n  props: [\"id\"],\r\n  data: () => ({\r\n    dataNews: [],\r\n    tabs: \"\",\r\n    loadingBtnSave: null,\r\n    status: null,\r\n    result: null,\r\n    loadingFields: null,\r\n    displayModalErrorText: null,\r\n    displayModalError: null,\r\n    textSuccessAlert: \"\",\r\n  }),\r\n\r\n  computed: {\r\n    editPermission() {\r\n      return this.hasPermission(this.$PermissionNames.permissionNewsEdit);\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    ...moduleNoticias.mapActions({\r\n      getNewsById: \"getById\",\r\n      updateNews: \"update\",\r\n    }),\r\n\r\n    async loadNewsItems() {\r\n      this.loadingFields = true;\r\n      let response = await this.getNewsById(this.id);\r\n      this.dataNews.push(response.data.result);\r\n      this.dataNews.push(response.data.result.children);\r\n      this.status = this.dataNews[0].status;\r\n      this.setImages();\r\n      this.loadingFields = false;\r\n    },\r\n\r\n    setImages() {\r\n      this.dataNews[0].imageSource = new File(\r\n        [this.dataNews[0].imageSource],\r\n        this.dataNews[0].imageSourceName,\r\n        {\r\n          type: \"base64\",\r\n        }\r\n      );\r\n\r\n      this.dataNews[1].imageSource = new File(\r\n        [this.dataNews[1].imageSource],\r\n        this.dataNews[1].imageSourceName,\r\n        {\r\n          type: \"base64\",\r\n        }\r\n      );\r\n    },\r\n\r\n    async submit() {\r\n      if (!this.$refs.form.validate()) return;\r\n\r\n      if (\r\n        this.dataNews[0].imageSource.size > 20000000 ||\r\n        this.dataNews[1].imageSource > 20000000\r\n      ) {\r\n        this.displayModalError = true;\r\n        this.displayModalErrorText =\r\n          \"Tamanho da imagem excede 20 Mb, selecione outra imagem para prosseguir!\";\r\n        setTimeout(() => {\r\n          this.displayModalError = false;\r\n        }, 5000);\r\n        this.loadingBtnSave = false;\r\n        return;\r\n      }\r\n      this.loadingBtnSave = true;\r\n      var items = {};\r\n      this.dataNews[0].language = \"pt\";\r\n      this.dataNews[0].status = this.status;\r\n      this.dataNews[1].language = \"en\";\r\n      this.dataNews[1].status = this.status;\r\n      if (this.dataNews[0].imageSource instanceof File) {\r\n        this.dataNews[0].imageSourceName = this.dataNews[0].imageSource.name;\r\n        this.dataNews[0].imageSource = await toBase64(\r\n          this.dataNews[0].imageSource,\r\n          false\r\n        );\r\n      }\r\n      if (this.dataNews[1].imageSource instanceof File) {\r\n        this.dataNews[1].imageSourceName = this.dataNews[1].imageSource.name;\r\n        this.dataNews[1].imageSource = await toBase64(\r\n          this.dataNews[1].imageSource,\r\n          false\r\n        );\r\n      }\r\n      items = this.dataNews;\r\n      let isEdit = false;\r\n      if (this.$route.params.id) {\r\n        this.result = await this.updateNews(items);\r\n        isEdit = true;\r\n      } else {\r\n        this.result = await this.create(items);\r\n      }\r\n\r\n      if (this.result.status == 200) {\r\n        this.throwSuccessMessage(isEdit);\r\n      } else {\r\n        this.displayModalError = true;\r\n        this.displayModalErrorText =\r\n          \"Não foi possível salvar, verifique os campos!\";\r\n        setTimeout(() => {\r\n          this.displayModalError = false;\r\n        }, 5000);\r\n      }\r\n\r\n      this.loadingBtnSave = false;\r\n    },\r\n\r\n    throwSuccessMessage(isEdit) {\r\n      if (isEdit) {\r\n        this.textSuccessAlert = \"Notícia editada com sucesso\";\r\n      } else {\r\n        this.textSuccessAlert = \"Notícia criada com sucesso\";\r\n      }\r\n\r\n      this.alertSuccess = true;\r\n      setTimeout(() => {\r\n        this.alertSuccess = false;\r\n        this.$router.push({ path: `/home/noticias` });\r\n      }, 3000);\r\n    },\r\n  },\r\n  mounted() {\r\n    if (this.id) {\r\n      this.loadNewsItems();\r\n    }\r\n    this.status = true;\r\n  },\r\n\r\n  mixins: [\r\n    getRouterData,\r\n    crudController({\r\n      store: \"ModuleNoticias\",\r\n    }),\r\n  ],\r\n\r\n  components: {\r\n    TopSection,\r\n    NewsLanguageTab,\r\n    Loading,\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.bg {\r\n  background-color: #f3f3f3;\r\n}\r\n\r\n.bg-hover {\r\n  background-color: #d4d4d4;\r\n}\r\n\r\n.v-tab {\r\n  text-transform: none !important;\r\n}\r\n</style>\r\n"]}]}