 :key=\"key\"\r\n        disable-sort\r\n        :headers=\"hasPermission($PermissionNames.permissionManufacturerEdit) ? \r\n          headers : headers.filter(x => x.value != 'col1')\"\r\n        :items=\"rows\"\r\n        :options.sync=\"options\"\r\n        :page=\"page\"\r\n        :server-items-length=\"totalItems\"\r\n        noDataText=\"Nenhum registro encontrado\"\r\n        :loading=\"tableLoading\"\r\n        loading-text=\"Aguarde, carregando fabricantes...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\"\r\n      >\r\n        <!-- Icone Edição -->\r\n        <template v-slot:[`item.col1`]=\"{ item }\">\r\n          <v-btn icon @click=\"updateItem(1, item.col1)\">\r\n            <v-icon> mdi-pencil </v-icon>\r\n          </v-btn>\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n\r\n    <!-- MODAL DE EDIÇÃO -->\r\n    <v-dialog persistent v-model=\"modalEdit\" max-width=\"800px\">\r\n      <div class=\"white pa-8 d-flex flex-column\">\r\n        <div\r\n          class=\"d-flex justify-space-between\"\r\n          style=\"border-bottom: 1px solid #e1e1e1\"\r\n        >\r\n          <h2 class=\"asia-neutral--text text--var-1 pb-2\">Editar Fabricante</h2>\r\n\r\n          <v-btn icon @click=\"closeModal()\">\r\n            <v-icon>mdi-close</v-icon>\r\n          </v-btn>\r\n        </div>\r\n\r\n        <v-alert\r\n          type=\"error\"\r\n          dense\r\n          class=\"mt-1\"\r\n          outlined\r\n          v-show=\"errorAlert\"\r\n          transition=\"fade-transition\"\r\n          >{{ errorAlertText }}</v-alert\r\n        >\r\n\r\n        <div class=\"d-flex flex-row mt-4 mb-4\">\r\n          <div>\r\n            <p\r\n              class=\"mb-0 asia-neutral--text text--var-3\"\r\n              style=\"font-size: 12px\"\r\n            >\r\n              Código\r\n            </p>\r\n            <p class=\"font-weight-medium mb-0\">{{ dataSelectedItem.code }}</p>\r\n          </div>\r\n\r\n          <div class=\"ml-10\">\r\n            <p\r\n              class=\"mb-0 asia-neutral--text text--var-3\"\r\n              style=\"font-size: 12px\"\r\n            >\r\n              Nome\r\n            </p>\r\n            <p class=\"font-weight-bold mb-0\">{{ dataSelectedItem.name }}</p>\r\n          </div>\r\n\r\n          <div class=\"ml-10\">\r\n            <p\r\n              class=\"mb-0 asia-neutral--text text--var-3\"\r\n              style=\"font-size: 12px\"\r\n            >\r\n              Razão Social\r\n            </p>\r\n            <p class=\"font-weight-bold mb-0\">\r\n              {{ dataSelectedItem.corporateName }}\r\n            </p>\r\n          </div>\r\n        </div>\r\n\r\n        <v-form ref=\"form\" v-model=\"valid\">\r\n          <v-text-field\r\n            v-model=\"dataSelectedItem.abreviation\"\r\n            label=\"Abreviação\"\r\n            v-mask=\"'XXXXXXXX'\"\r\n            :rules=\"[rules.required]\"\r\n            :loading=\"fieldLoading\"\r\n          ></v-text-field>\r\n        </v-form>\r\n\r\n        <div class=\"d-flex flex-row justify-end pt-5\">\r\n          <v-btn\r\n            rounded\r\n            large\r\n            class=\"asia-red white--text\"\r\n            @click=\"updateItem(2)\"\r\n            :loading=\"loadingBtnSave\"\r\n            >SALVAR</v-btn\r\n          >\r\n        </div>\r\n      </div>\r\n    </v-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\nimport crudController from \"@/mixins/crudController.js\";\r\nimport validationRules from \"../../mixins/validationRules\";\r\nimport { mask } from 'vue-the-mask'\r\n\r\nexport default {\r\n  directives: { mask },\r\n\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"col1\", width: \"5%\" },\r\n      { text: \"Código\", value: \"col2\", width: \"8%\" },\r\n      { text: \"Nome\", value: \"col3\", width: \"35%\" },\r\n      { text: \"Razão Social\", value: \"col4\", width: \"35%\" },\r\n      { text: \"Abreviação\", value: \"col5\", width: \"\" },\r\n    ],\r\n    rows: [],\r\n  }),\r\n  methods: {\r\n    buildRows(data) {\r\n      this.rows = data.map((e) => ({\r\n        col1: e.id,\r\n        col2: e.code,\r\n        col3: e.name,\r\n        col4: e.corporateName,\r\n        col5: e.abreviation,\r\n      }));\r\n    },\r\n\r\n    loadEditionData(data) {\r\n      this.dataSelectedItem = data;\r\n    },\r\n  },\r\n  mixins: [\r\n    validationRules,\r\n    getRouterData,\r\n    dataTable({ store: \"ModuleFabricantes\" }),\r\n    crudController({ store: \"ModuleFabricantes\" }),\r\n  ],\r\n  components: { TopSection, ActionsTable },\r\n};\r\n</script>\r\n\r\n<style>\r\n</style>"]}]}