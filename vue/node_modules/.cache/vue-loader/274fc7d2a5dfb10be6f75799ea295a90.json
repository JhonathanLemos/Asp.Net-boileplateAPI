ado\"\r\n        :loading=\"tableLoading\"\r\n        loading-text=\"Aguarde, carregando noticias...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\"\r\n      >\r\n        <!-- Icone Edição -->\r\n        <template v-slot:[`item.col1`]=\"{ item }\">\r\n          <v-btn v-if=\"editPermission\" icon @click=\"$router.push(`noticias/edit/${item.col1}`)\">\r\n            <v-icon> mdi-pencil </v-icon>\r\n          </v-btn>\r\n          <v-btn v-else icon @click=\"$router.push(`noticias/edit/${item.col1}`)\">\r\n            <v-icon> mdi-eye </v-icon>\r\n          </v-btn>\r\n        </template>\r\n\r\n        <!-- LastModification -->\r\n        <template v-slot:[`item.col3`]=\"{ item }\">\r\n          <p class=\"mt-3\">\r\n            {{ item.col3 | formatDate }}\r\n          </p>\r\n        </template>\r\n\r\n        <!-- Status -->\r\n        <template v-slot:[`item.col4`]=\"{ item }\">\r\n          <v-chip :color=\"item.col4.color\" outlined>\r\n            {{ item.col4.text | AtivoInativo }}\r\n          </v-chip>\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\n\r\nexport default {\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"col1\", width: \"10%\" },\r\n      { text: \"Titulo\", value: \"col2\", width: \"40%\" },\r\n      { text: \"Data de Alteração\", value: \"col3\", width: \"45%\" },\r\n      { text: \"Status\", value: \"col4\", width: \"\" },\r\n    ],\r\n    rows: [],\r\n  }),\r\n\r\n  computed: {\r\n    editPermission() {\r\n      return this.hasPermission(this.$PermissionNames.permissionNewsEdit)\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    buildRows(data) {\r\n      let dataRows = [];\r\n      data.forEach((e) => \r\n      {\r\n        dataRows.push({\r\n          col1: e.id,\r\n          col2: e.title,\r\n          col3: e.lastModificationTime,\r\n          col4: {\r\n            text: e.status,\r\n            color: e.status === true ? \"success\" : \"grey\",\r\n          },\r\n        });\r\n      });\r\n      this.rows = dataRows;\r\n    },\r\n  },\r\n\r\n  mixins: [getRouterData, dataTable({ store: \"ModuleNoticias\" })],\r\n  components: {\r\n    TopSection,\r\n    ActionsTable,\r\n  },\r\n};\r\n</script>"]}]}