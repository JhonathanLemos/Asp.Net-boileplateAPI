 >mdi-alert</v-icon\r\n        >\r\n\r\n        <div class=\"d-flex align-start\">\r\n          <h4 class=\"myApi-neutral--text text--var-1 pb-5 pt-5 mb-4\">\r\n            Você está prestes a remover um grupo/ <br />\r\n            cliente a um usuário, esse processo <br />\r\n            retira as permissões de acesso do <br />\r\n            usuário a esse grupo/cliente. <br />\r\n            Deseja continuar?\r\n          </h4>\r\n        </div>\r\n\r\n        <div class=\"d-flex flex-row justify-end\">\r\n          <v-btn\r\n            rounded\r\n            large\r\n            text\r\n            class=\"mr-6\"\r\n            @click=\"dialogDesvincularConfirm = false\"\r\n            >CANCELAR</v-btn\r\n          >\r\n\r\n          <v-btn\r\n            rounded\r\n            large\r\n            class=\"white--text\"\r\n            style=\"background: #f75a02\"\r\n            @click=\"removeFromList('', 2)\"\r\n            >REMOVER VÍNCULO</v-btn\r\n          >\r\n        </div>\r\n      </div>\r\n    </v-dialog>\r\n    <!-- FIM -->\r\n\r\n    <div style=\"overflow-y: auto\" class=\"rounded-lg pa-4 white\">\r\n      <!-- DATA TABLE -->\r\n      <v-data-table\r\n        disable-sort\r\n        :headers=\"readonly ? \r\n          headers.filter(x => x.value != 'col1') : headers\"\r\n        :items=\"rows\"\r\n        :page=\"page\"\r\n        :server-items-length=\"rows.length\"\r\n        noDataText=\"Nenhum registro encontrado\"\r\n        loading-text=\"Aguarde, carregando usuários...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\"\r\n      >\r\n        <!-- Icone Manutenção -->\r\n\r\n        <template v-slot:[`item.col1`]=\"{ item }\">\r\n          <div class=\"d-flex flex-nowrap\">\r\n            <v-btn icon @click=\"removeFromList(item, 1)\">\r\n              <v-icon>mdi-link-variant-remove</v-icon>\r\n            </v-btn>\r\n\r\n            <v-btn icon @click=\"editGroup(item)\">\r\n              <v-icon> mdi-pencil </v-icon>\r\n            </v-btn>\r\n          </div>\r\n        </template>\r\n\r\n        <!-- Status -->\r\n        <template v-slot:[`item.col4`]=\"{ item }\">\r\n          <v-chip :color=\"item.col4.color\" outlined style=\"font-weight: 500\">\r\n            {{ item.col4.text }}\r\n          </v-chip>\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  props: { items: null, groupLength: null, reset: null, readonly: Boolean },\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"col1\", width: \"15%\" },\r\n      { text: \"Grupo\", value: \"col2\" },\r\n      { text: \"Clientes Liberados\", value: \"col3\" },\r\n      { text: \"Situação\", value: \"col4\" },\r\n    ],\r\n    page: 1,\r\n    idToDelete: null,\r\n    rows: [],\r\n    dialogDesvincularConfirm: false,\r\n  }),\r\n\r\n  watch: {\r\n    items() {\r\n      this.setItems(this.items);\r\n    },\r\n    reset() {\r\n      if (this.reset) {\r\n        this.resetMethod();\r\n      }\r\n    },\r\n  },\r\n\r\n  methods: {\r\n    resetMethod() {\r\n      this.setItems(\"reset\");\r\n    },\r\n    removeFromList(item, v) {\r\n      if (v == 1) {\r\n        this.idToDelete = item.col1;\r\n        this.dialogDesvincularConfirm = true;\r\n      } else {\r\n        this.$emit(\"removeItemFromCustomersGroupList\", this.idToDelete);\r\n        this.dialogDesvincularConfirm = false;\r\n      }\r\n    },\r\n    async setItems(items) {\r\n      if (items == \"reset\") {\r\n        this.rows = [];\r\n        this.$emit(\"reset\", false);\r\n        return;\r\n      }\r\n      if (items.length == 0) return;\r\n      let dataRows = [];\r\n      items.forEach((e) => {\r\n        var length =\r\n          e.customers.length > 0 || e.customers.length < 10\r\n            ? \"0\" + e.customers.length\r\n            : e.customers.length;\r\n        var groupLength =\r\n          e.groupLength > 0 || e.groupLength < 10\r\n            ? \"0\" + e.groupLength\r\n            : e.groupLength;\r\n        dataRows.push({\r\n          col1: e.groupId,\r\n          col2: e.groupName,\r\n          col3: length + \" / \" + groupLength,\r\n          col4: {\r\n            text: e.status === true ? \"Ativo\" : \"Inativo\",\r\n            color: e.status === true ? \"success\" : \"grey\",\r\n          },\r\n        });\r\n      });\r\n\r\n      this.rows = dataRows;\r\n    },\r\n\r\n    editGroup(item) {\r\n      this.$emit(\"editarGroupInTable\", item);\r\n    },\r\n  },\r\n\r\n  mounted() {\r\n    if (this.items) this.setItems(this.items);\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.grupos:hover {\r\n  background-color: #fae7e7;\r\n  border-left: 2px solid #a10a0c;\r\n  background-image: mdi-menu-right;\r\n}\r\n</style>\r\n"]}]}