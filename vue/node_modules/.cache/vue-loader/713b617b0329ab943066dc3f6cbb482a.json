"Pesquisar por Proforma ou Cliente\"\r\n    />\r\n\r\n    <div\r\n      style=\"overflow-y: auto\"\r\n      class=\"flex-grow-1 rounded-lg white elevation-5\"\r\n    >\r\n      <v-data-table\r\n        ref=\"dataTable\"\r\n        style=\"font-size: 10px\"\r\n        :key=\"key\"\r\n        disable-sort\r\n        :headers=\"headers\"\r\n        :items=\"rows\"\r\n        :options.sync=\"options\"\r\n        :page=\"page\"\r\n        :server-items-length=\"totalItems\"\r\n        noDataText=\"Nenhum registro encontrado\"\r\n        :loading=\"tableLoading\"\r\n        loading-text=\"Aguarde, carregando proformas...\"\r\n        :footer-props=\"{\r\n          itemsPerPageOptions: [10, 50, 100],\r\n          itemsPerPageText: 'Itens por Página',\r\n        }\"\r\n        show-expand\r\n      >\r\n        <template v-slot:[`item.id`]=\"{ item }\">\r\n          <v-btn\r\n            v-if=\"hasPermission($PermissionNames.permissionProformaEdit)\"\r\n            icon\r\n            @click=\"redirectToEdit(item.id)\"\r\n          >\r\n            <v-icon> mdi-pencil </v-icon>\r\n          </v-btn>\r\n          <v-btn v-else icon @click=\"redirectToEdit(item.id)\">\r\n            <v-icon> mdi-eye </v-icon>\r\n          </v-btn>\r\n        </template>\r\n\r\n        <template v-slot:[`item.pendency`]=\"{ item }\">\r\n          <v-row justify=\"center\">\r\n            <v-icon v-if=\"item.pendency\" color=\"orange darken-2\">\r\n              mdi-alert\r\n            </v-icon>\r\n          </v-row>\r\n        </template>\r\n\r\n        <template v-slot:expanded-item=\"{ headers, item }\">\r\n          <td :colspan=\"headers.length\">\r\n            <v-container class=\"text-left pl-7 ml-7\">\r\n              <v-row>\r\n                <b class=\"mr-2\">Embarques</b>\r\n                <div\r\n                  class=\"ml-3\"\r\n                  v-for=\"(boarding, i) in item.boardingGroup\"\r\n                  :key=\"i\"\r\n                >\r\n                  <b>{{ `${i + 1}° ` }}</b>\r\n                  {{ boarding.invoice }}\r\n                </div>\r\n              </v-row>\r\n            </v-container>\r\n          </td>\r\n        </template>\r\n\r\n        <template v-slot:[`item.shipmentLimitDate`]=\"{ item }\">\r\n          {{ item.shipmentLimitDate | dateFormat }}\r\n        </template>\r\n\r\n        <template v-slot:[`item.proformaStatus`]=\"{ item }\">\r\n          <category-chip\r\n            v-if=\"item.proformaStatus\"\r\n            :color=\"\r\n              item.proformaStatus.color == '#FFFFFF'\r\n                ? 'black'\r\n                : item.proformaStatus.color\r\n            \"\r\n            :text=\"item.proformaStatus.description\"\r\n          />\r\n        </template>\r\n      </v-data-table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport TopSection from \"@/components/TopSection.vue\";\r\nimport ActionsTable from \"@/components/ActionsTable.vue\";\r\nimport getRouterData from \"@/mixins/getRouterData.js\";\r\nimport dataTable from \"@/mixins/dataTable.js\";\r\nimport crudController from \"@/mixins/crudController.js\";\r\nimport CategoryChip from \"@/components/CategoryChip.vue\";\r\n\r\nexport default {\r\n  data: () => ({\r\n    headers: [\r\n      { text: \"\", value: \"id\" },\r\n      { text: \"\", value: \"data-table-expand\" },\r\n      { text: \"Proforma\", value: \"proformaValue\", width: \"10%\" },\r\n      { text: \"Pendência\", value: \"pendency\" },\r\n      { text: \"Cliente\", value: \"customerName\", width: \"23%\" },\r\n      { text: \"Fabricante\", value: \"manufacturerName\", width: \"23%\" },\r\n      { text: \"Exportador\", value: \"exporterName\", width: \"23%\" },\r\n      { text: \"Data Limite\", value: \"shipmentLimitDate\", width: \"8%\" },\r\n      {\r\n        text: \"Situação\",\r\n        value: \"proformaStatus\",\r\n        width: \"10%\",\r\n        align: \"center\",\r\n      },\r\n    ],\r\n    rows: [],\r\n  }),\r\n\r\n  methods: {\r\n    async buildRows(data) {\r\n      this.rows = data;\r\n    },\r\n    redirectToEdit(id) {\r\n      let params = {\r\n        id: id,\r\n        options: this.$refs.dataTable.options,\r\n      };\r\n      this.$router.push({ name: \"ViewProformasEdit\", params: params });\r\n    },\r\n  },\r\n\r\n  mixins: [\r\n    getRouterData,\r\n    dataTable({ store: \"ModuleProformas\" }),\r\n    crudController({ store: \"ModuleProformas\" }),\r\n  ],\r\n  components: { TopSection, ActionsTable, CategoryChip },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.v-data-table__expanded__content {\r\n  box-shadow: none;\r\n  td {\r\n    background-color: #e6e6e6;\r\n  }\r\n}\r\n</style>\r\n"]}]}